function getGreeting() {
    $time = date("H");
    if ($time < "12") {
        return "Good morning";
    } else {
        if ($time >= "12" && $time < "17") {
            return "Good afternoon";
        } else {
            if ($time >= "17" && $time < "19") {
                return "Good evening";
            } else
                if ($time >= "19") {
                    return "Good night";
                }
        }
    }
}

